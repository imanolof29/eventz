    "handlebars": "^4.7.8",
    "heic-convert": "^2.1.0",
    "moment": "^2.30.1",
    "nodemailer": "^6.9.16",
    "nodemailer-express-handlebars": "^7.0.0",
    "passport": "^0.7.0",
    "passport-google-oauth2": "^0.2.0",
    "passport-jwt": "^4.0.1",
    "pg": "^8.13.0",
